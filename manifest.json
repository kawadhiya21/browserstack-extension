{
  "manifest_version": 2,

  "name": "Browser Stack Ext",
  "description": "Launch any page in any OS and browser combination",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://www.browserstack.com/"
  ]
}